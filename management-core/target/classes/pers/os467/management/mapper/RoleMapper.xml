<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="pers.os467.management.mapper.RoleMapper">


    <select id="getRoleByRid" resultType="pers.os467.management.entity.Role">
        select * from role where rid = #{rid};
    </select>
    <select id="getRoleList" resultType="pers.os467.management.entity.Role">
        select * from role
    </select>
</mapper>